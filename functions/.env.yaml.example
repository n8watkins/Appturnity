# Firebase Functions Environment Variables (YAML format)
# Copy this file to .env.yaml and fill in your actual values
# This file is used when deploying to Firebase Functions

# Resend API Configuration
RESEND_API_KEY: your_resend_api_key_here
RESEND_FROM_EMAIL: "Your Name <your@email.com>"

# Contact Email
CONTACT_EMAIL: your_contact_email@example.com

# reCAPTCHA Secret Key
RECAPTCHA_SECRET_KEY: your_recaptcha_secret_key_here

# Frontend URL (for CORS)
FRONTEND_URL: https://your-app.web.app
